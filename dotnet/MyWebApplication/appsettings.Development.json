{
  "ConnectionStrings": {
    "MyDatabase": "Server=LOCALHOST;Database=MyDatabase;Integrated Security=True;Application Name=MyWebApplicationDevelopment;Encrypt=False;Connect Timeout=1;Command Timeout=0;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "MyWebApplication": "Verbose",
        "MyClassLibrary.MyRepository": "Verbose",
        "MyClassLibrary.MyService": "Verbose"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Level:u3}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}
